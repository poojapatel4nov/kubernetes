    "import pickle\n",
    "flower_dic = {1:'rose', 2:'lily',3:'lotus',4:'tulip'}\n",
    "filename = 'flower.txt'\n",
    "outfile = open(filename,'wb')\n",
    "pickle.dump(flower_dic, outfile)\n",
    "outfile.close()\n",
    "\n",
    "infile = open(filename,'rb')\n",
    "new_dic = pickle.load(infile)\n",
    "infile.close()\n",
    "print(new_dic)\n",
    "print(type(new_dic))\n",
    "\n"
